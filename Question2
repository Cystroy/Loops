package loops;

public class Question2 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	int i = 1;
	int j = 1;

		for (i = 1; i <= 10; i++) { 
			for (j = 1 ; j <= i; j++) { 
	    System.out.print(j + " "); }  
			System.out.println(" "); } 
	
		//The output of the loops displays a triangle of numbers. The number 10 at the bottom of a slope and counts down to one going up the slope.
}
}


